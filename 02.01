package main

import (
    "fmt") 
    
type americanVelocity float64
type europianVelocity float64

var Aspeed americanVelocity
var Espeed europianVelocity

func americanSpeed (data americanVelocity) {
    cof:= 2.237
    speed := float64(data)*cof
    Aspeed = americanVelocity(speed)
    fmt.Println(Aspeed, "миль/час")
}

func europianSpeed (data europianVelocity){
    var cof europianVelocity= 3.588
    Espeed = data*cof
    fmt.Println(Espeed, "километры/час")
}

func main() {
    var dataA americanVelocity = 130
    americanSpeed(dataA)
    var dataE europianVelocity = 120.4
    europianSpeed(dataE)
}
